= form_for(@ticket_holder) do |f|
  - if @ticket_holder.errors.any?
    #error_explanation
      %h2
        = pluralize(@ticket_holder.errors.count, "error")
        prohibited this ticket_holder from being saved:
      %ul
        - @ticket_holder.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :season_id
    %br/
    = f.select :season_id, @seasons.collect { |s| [ s.years, s.id ] }
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .actions
    = f.submit
